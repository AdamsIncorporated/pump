{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "cppvsdbg",
      "request": "launch",
      "name": "Debug Pump - Backend",
      "program": "${workspaceFolder}/backend/target/debug/pump.exe",
      "args": [],
      "cwd": "${workspaceRoot}/backend",
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnSessionStart",
      "stopAtEntry": false
    },
    {
      "name": "Pump Frontend",
      "type": "firefox",
      "request": "launch",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}/frontend"
    }
  ]
}
