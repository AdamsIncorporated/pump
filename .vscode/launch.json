{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Pump Backend",
      "type": "lldb",
      "request": "launch",
      "cargo": {
        "args": ["build", "--package=pump", "--bin=pump"],
        "filter": {
          "name": "pump",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}/backend/"
    },
    {
      "name": "Pump Frontend",
      "type": "firefox",
      "request": "launch",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}/frontend"
    }
  ]
}
